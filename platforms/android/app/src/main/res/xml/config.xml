<?xml version='1.0' encoding='utf-8'?>
<widget id="dev.oscarsalguero.reversertest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="reverserplugin">
        <param name="android-package" value="dev.oscarsalguero.reverserplugin.reverserplugin" />
    </feature>
    <name>Reverser</name>
    <description>Simple Apache Cordova Test App for the Reverser Plugin.</description>
    <author email="info@oscarsalguero.com" href="https://oscarsalguero.dev">
        Oscar Salguero
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon density="mdpi" src="res/icons/android/mdpi.png" />
    <icon density="hdpi" src="res/icons/android/hdpi.png" />
    <icon density="xhdpi" src="res/icons/android/xhdpi.png" />
    <icon density="xxhdpi" src="res/icons/android/xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icons/android/xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="ReverserPlugin">
        <param name="android-package" value="dev.oscarsalguero.reverserplugin.ReverserPlugin" />
    </feature>
</widget>
